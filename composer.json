{
	"autoload" : {
		"psr-4" : {
			"Hospitalplugin\\" : "src"
		}
	},
	"name" : "amarcinkowski/hospitalplugin",
	"type" : "library",
	"description" : "Hospital plugins library",
	"license": "MIT",
	"require" : {
		"php" : ">=5.5",
		"doctrine/orm" : "*",
		"symfony/yaml" : "*",
		"vlucas/phpdotenv" : "~1.0.6",
		"twig/twig" : "*"
	},
	"require-dev" : {
		"satooshi/php-coveralls" : "dev-master"
	},
	"scripts" : {
		"post-install-cmd" : [
			"bower install"
		],
		"post-update-cmd" : [
			"bower install"
		]
	},
	"db" : "vendor/bin/doctrine orm:schema-tool:update --force --dump-sql"
}